<p id="notice"><%= notice %></p>

<h1>Listing Solicitacoes</h1>

<table>
  <thead>
    <tr>
      <th>Instituicao</th>
      <th>Medico</th>
      <th>Clinica</th>
      <th>Paciente</th>
      <th>Solicitante</th>
      <th>Data emissao</th>
      <th>Data agendamento</th>
      <th>Hora agendamento</th>
      <th>Status</th>
      <th>Descricao</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @solicitacoes.each do |solicitacao| %>
      <tr>
        <td><%= solicitacao.instituicao_id %></td>
        <td><%= solicitacao.medico_id %></td>
        <td><%= solicitacao.clinica_id %></td>
        <td><%= solicitacao.paciente_id %></td>
        <td><%= solicitacao.solicitante %></td>
        <td><%= solicitacao.data_emissao %></td>
        <td><%= solicitacao.data_agendamento %></td>
        <td><%= solicitacao.hora_agendamento %></td>
        <td><%= solicitacao.status %></td>
        <td><%= solicitacao.descricao %></td>
        <td><%= link_to 'Show', solicitacao %></td>
        <td><%= link_to 'Edit', edit_solicitacao_path(solicitacao) %></td>
        <td><%= link_to 'Destroy', solicitacao, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Solicitacao', new_solicitacao_path %>
