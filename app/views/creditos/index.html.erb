<p id="notice"><%= notice %></p>

<h1>Listing Creditos</h1>

<table>
  <thead>
    <tr>
      <th>Medico</th>
      <th>Data emissao</th>
      <th>Credito</th>
      <th>Saldo</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @creditos.each do |credito| %>
      <tr>
        <td><%= credito.medico_id %></td>
        <td><%= credito.data_emissao %></td>
        <td><%= credito.credito %></td>
        <td><%= credito.saldo %></td>
        <td><%= link_to 'Show', credito %></td>
        <td><%= link_to 'Edit', edit_credito_path(credito) %></td>
        <td><%= link_to 'Destroy', credito, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Credito', new_credito_path %>
